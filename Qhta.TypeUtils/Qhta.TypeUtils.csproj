<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <BaseOutputPath>D:\VS\Projects\MyLib\bin</BaseOutputPath>
    <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RootNamespace>Qhta.TypeUtils</RootNamespace>
    <Description>A package of functions to supplement System.Reflections.</Description>
    <Authors>Yarreck Qhta</Authors>
    <Version>1.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Qhta.Collections">
      <HintPath>..\bin\Debug\net6.0\Qhta.Collections.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
