              </Grid>
              <local:PropertyGridSplitter p9:Grid.Row="2" p9:FrameworkElement.VerticalAlignment="Center" p9:Control.BorderBrush="#FF35496A" p9:Control.BorderThickness="0 1 0 0" p9:Control.Background="#FFC2CCDB" p9:FrameworkElement.HorizontalAlignment="Stretch" p9:FrameworkElement.Height="3" p9:UIElement.Visibility="{TemplateBinding local:PropertyGrid.DescriptionPanelVisibility}" xmlns:p9="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:local="clr-namespace:Syncfusion.Windows.PropertyGrid;assembly=Syncfusion.PropertyGrid.Wpf,Version=29.1462.33.0,Culture=neutral,PublicKeyToken=3d67ed1f87d44c89" />
              <local:DescriptionBorder p9:Grid.Row="3" p9:UIElement.Visibility="{TemplateBinding local:PropertyGrid.DescriptionPanelVisibility}" p9:Border.BorderBrush="{TemplateBinding Control.BorderBrush}" p9:Border.BorderThickness="0 1 0 0" xmlns:p9="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:local="clr-namespace:Syncfusion.Windows.PropertyGrid;assembly=Syncfusion.PropertyGrid.Wpf,Version=29.1462.33.0,Culture=neutral,PublicKeyToken=3d67ed1f87d44c89">
                <StackPanel>
