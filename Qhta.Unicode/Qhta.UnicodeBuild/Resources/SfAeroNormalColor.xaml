                  </Grid>
                </Border>
              </Grid>
              <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition ColumnDefinition.Width="Auto" />
                  <ColumnDefinition ColumnDefinition.Width="*" />
                </Grid.ColumnDefinitions>
                <Border x:Name="Sideborder" Border.Background="#FFF0F0F0" FrameworkElement.Width="14" UIElement.Visibility="Collapsed" />
