                      <ContentControl.Content>
                        <Path FrameworkElement.Name="pathButton" UIElement.SnapsToDevicePixels="True" Shape.Stroke="{TemplateBinding Control.Foreground}" Shape.StrokeThickness="1.5" Path.Data="M1,1L7,7 M1,7L7,1" FrameworkElement.HorizontalAlignment="Center" FrameworkElement.VerticalAlignment="Center" FrameworkElement.Width="8" FrameworkElement.Height="8" />
                      </ContentControl.Content>
