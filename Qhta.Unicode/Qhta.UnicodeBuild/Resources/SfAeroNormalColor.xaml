            <Trigger Trigger.Property="local:PropertyGrid.EnableGrouping" Trigger.Value="True">
              <Setter Setter.TargetName="Sideborder" Setter.Property="UIElement.Visibility" Setter.Value="Collapsed" />
            </Trigger>
            <Trigger Trigger.Property="local:PropertyGrid.PropertyExpandMode" Trigger.Value="NestedMode">
              <Setter Setter.TargetName="Sideborder" Setter.Property="UIElement.Visibility" Setter.Value="Collapsed" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="{x:Type local:PropertyGrid}" Style.BasedOn="{StaticResource DefaultPropertyGridStyle}" Style.TargetType="{x:Type local:PropertyGrid}" />
  <Style x:Key="{x:Type local:ItemsSourceControl}" Style.TargetType="{x:Type local:ItemsSourceControl}">
    <Setter Setter.Property="Control.Template">
      <Setter.Value>
        <ControlTemplate ControlTemplate.TargetType="{x:Type local:ItemsSourceControl}">
          <Border Border.Background="{TemplateBinding Control.Background}" Border.BorderBrush="{TemplateBinding Control.BorderBrush}" Border.BorderThickness="{TemplateBinding Control.BorderThickness}">
            <Grid>
              <Grid.ColumnDefinitions>
                <ColumnDefinition ColumnDefinition.Width="*" />
                <ColumnDefinition ColumnDefinition.Width="Auto" />
              </Grid.ColumnDefinitions>
              <TextBlock TextBlock.Name="Part_Text" FrameworkElement.Margin="2,0" FrameworkElement.VerticalAlignment="Center" Grid.Column="0" TextBlock.Text="{local:PropertyGridLocalizationResource ResourceName=CollectionEditorWatermark}" />
              <Button Button.Name="Part_Btn" Grid.Column="1" ContentControl.Content="..." />
            </Grid>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="DefaultMenuButtonStyle" Style.TargetType="{x:Type local:DefaultMenuButton}">
    <Setter Setter.Property="FrameworkElement.Width" Setter.Value="7" />
    <Setter Setter.Property="FrameworkElement.Height" Setter.Value="7" />
    <Setter Setter.Property="Control.Template">
      <Setter.Value>
        <ControlTemplate ControlTemplate.TargetType="{x:Type local:DefaultMenuButton}">
          <Grid>
            <Border Border.Background="#00FFFFFF" Border.BorderBrush="#FF000000">
              <Rectangle Rectangle.Name="rectangle" Shape.Stroke="#FF000000" />
            </Border>
            <Popup FrameworkElement.Name="PART_Popup" Popup.Placement="Left" UIElement.Focusable="False" Popup.PopupAnimation="Fade" Popup.AllowsTransparency="True" Popup.StaysOpen="False" Popup.IsOpen="False">
              <Microsoft_Windows_Themes:SystemDropShadowChrome Microsoft_Windows_Themes:SystemDropShadowChrome.Name="PART_Shadow" p9:UIElement.SnapsToDevicePixels="True" xmlns:p9="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero">
                <Border Border.Background="{StaticResource ViewBackgroundColor}" FrameworkElement.MinHeight="14" FrameworkElement.MinWidth="28" Border.BorderThickness="1" Border.BorderBrush="{DynamicResource {x:Static SystemColors.WindowFrameBrushKey}}">
                  <Grid>
                    <StackPanel FrameworkElement.Margin="2">
                      <Border Border.BorderThickness="0,0,0,1" FrameworkElement.Margin="0,0,0,1" FrameworkElement.Height="22" FrameworkElement.Width="auto">
                        <TextBlock FrameworkElement.Margin="3,0,0,0" TextBlock.FontWeight="Bold" FrameworkElement.VerticalAlignment="Center" FrameworkElement.Width="auto" TextBlock.Text="{Binding DisplayName}" />
                      </Border>
                      <MenuItem MenuItem.Name="Reset_MenuItem" HeaderedItemsControl.Header="{local:PropertyGridLocalizationResource ResourceName=Reset}" MenuItem.Command="{Binding ResetCommand}" />
                      <Separator FrameworkElement.Name="TopSeperator" UIElement.Visibility="Collapsed" FrameworkElement.Margin="30,0,0,0" FrameworkElement.Height="2" />
                      <ItemsControl FrameworkElement.Name="PART_Menu" Control.VerticalContentAlignment="Top" Control.HorizontalContentAlignment="Center" KeyboardNavigation.IsTabStop="False" FocusManager.IsFocusScope="False" />
                    </StackPanel>
                  </Grid>
                </Border>
              </Microsoft_Windows_Themes:SystemDropShadowChrome>
            </Popup>
          </Grid>
          <ControlTemplate.Triggers>
            <DataTrigger DataTrigger.Value="True" DataTrigger.Binding="{Binding IsDefaultValue}">
              <Setter Setter.TargetName="rectangle" Setter.Property="Shape.Fill" Setter.Value="#FF000000" />
            </DataTrigger>
            <Trigger Trigger.SourceName="PART_Menu" Trigger.Property="ItemsControl.HasItems" Trigger.Value="True">
              <Setter Setter.TargetName="TopSeperator" Setter.Property="UIElement.Visibility" Setter.Value="Visible" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="{x:Type local:DefaultMenuButton}" Style.BasedOn="{StaticResource DefaultMenuButtonStyle}" Style.TargetType="{x:Type local:DefaultMenuButton}" />
</ResourceDictionary>



