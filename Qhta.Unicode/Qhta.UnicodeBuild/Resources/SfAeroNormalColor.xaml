                </StackPanel>
              </local:DescriptionBorder>
            </Grid>
          </Border>
          <ControlTemplate.Triggers>
            <Trigger Trigger.Property="local:PropertyGrid.CollapseSidePanel" Trigger.Value="False">
              <Setter Setter.TargetName="Sideborder" Setter.Property="UIElement.Visibility" Setter.Value="Visible" />
