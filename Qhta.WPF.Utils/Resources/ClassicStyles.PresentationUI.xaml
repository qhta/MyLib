                    xmlns:ui="System.Windows.Documents;assembly=PresentationUI"
                    >
    <SolidColorBrush x:Key="(ui:PresentationUIStyleResources).StickyNoteDefaultBackgroundBrush" x:Uid="SolidColorBrush_1" SolidColorBrush.Color="{StaticResource {x:Static SystemColors.InfoColorKey}}" />
    <SolidColorBrush x:Key="(ui:PresentationUIStyleResources).DocumentApplicationDefaultBackgroundBrush" x:Uid="SolidColorBrush_2" SolidColorBrush.Color="{StaticResource {x:Static SystemColors.ControlColorKey}}" />
  </ResourceDictionary>

 

 
