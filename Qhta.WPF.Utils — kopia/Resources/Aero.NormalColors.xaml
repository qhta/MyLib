  <Style x:Key="{x:Type CalendarDayButton}" Style.TargetType="{x:Type CalendarDayButton}" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Setter Setter.Property="MinWidth" Setter.Value="5" />
    <Setter Setter.Property="MinHeight" Setter.Value="5" />
    <Setter Setter.Property="FontSize" Setter.Value="10" />
    <Setter Setter.Property="HorizontalContentAlignment" Setter.Value="Center" />
    <Setter Setter.Property="VerticalContentAlignment" Setter.Value="Center" />
