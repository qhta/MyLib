    <Setter Setter.Property="Template">
      <Setter.Value>
        <ControlTemplate>
          <Rectangle Margin="2" Shape.StrokeThickness="1" Shape.Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" Shape.StrokeDashArray="1 2" SnapsToDevicePixels="True" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <LinearGradientBrush x:Key="ButtonBaseBackground" LinearGradientBrush.StartPoint="0,0" LinearGradientBrush.EndPoint="0,1" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <GradientBrush.GradientStops>
      <GradientStop GradientStop.Color="#F3F3F3" GradientStop.Offset="0" />
      <GradientStop GradientStop.Color="#EBEBEB" GradientStop.Offset="0.5" />
      <GradientStop GradientStop.Color="#DDDDDD" GradientStop.Offset="0.5" />
      <GradientStop GradientStop.Color="#CDCDCD" GradientStop.Offset="1" />
    </GradientBrush.GradientStops>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="ButtonBaseBorderBrush" SolidColorBrush.Color="#FF707070" />
