<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Imagin.Common"
    xmlns:Common="http://imagin.tech/imagin/common"
    xmlns:Controls.Common="http://imagin.tech/imagin/controls/common">
    <Style x:Key="{x:Type local:AnglePicker}" TargetType="{x:Type local:AnglePicker}">
        <Setter Property="Background" Value="{DynamicResource Brush.AnglePicker.Background}"/>
        <Setter Property="NeedleStroke" Value="{DynamicResource Brush.AnglePicker.NeedleStroke}"/>
        <Setter Property="OriginFill" Value="{DynamicResource Brush.AnglePicker.OriginFill}"/>
        <Setter Property="OriginStroke" Value="{DynamicResource Brush.AnglePicker.OriginStroke}"/>
    </Style>
</ResourceDictionary>